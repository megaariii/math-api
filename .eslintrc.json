{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-trailing-spaces": "off",
    "comma-dangle": "off",
    "no-underscore-dangle": "off",
    "function-paren-newline": "off",
    "implicit-arrow-linebreak": "off"
  }
}
